import java.util.Scanner;
class KeithNum 
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter Num : ");
		int num = sc.nextInt();

		int sum = 0;
		while(sum < num){
			for(int i = num ; i>0; i/=10){
				sum += (i%10);
			}
			System.out.println(sum);
		}
	}
}
